<plugin>

   <extension
         id="SysMLv2ObjectMarker"
         name="SysMLv2 Model Object Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <super
            type="org.eclipse.emf.ecore.diagnostic">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         id="SysMLv2GotoMarker"
         name="Goto Model Element Marker (SysMLv2)"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.osate.sysmlv2.contract.SysMLv2ObjectMarker">
      </super>
      <super
            type="org.eclipse.emf.ecore.diagnostic">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         id="SysMLv2TextMarker"
         name="SysMLv2 Text Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         id="SysMLv2TextGotoMarker"
         name="Goto SysMLv2 Source Text Marker"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.osate.sysmlv2.contract.SysMLv2TextMarker">
      </super>
      <persistent
            value="false">
      </persistent>
   </extension>

   <extension
         id="ElementMarker"
         name="SysMLv2 Element Marker"
         point="org.eclipse.core.resources.markers">
      <super type="org.osate.sysmlv2.contract.SysMLv2ObjectMarker"/>
      <persistent value="true"/>
   </extension>

	<extension point="org.eclipse.emf.ecore.generated_package">
		<package
			uri = "http://www.osate.org/contract/ContractForSysMLv2"
			class = "org.osate.sysmlv2.contract.contract.ContractPackage"
			genModel = "model/generated/Contract.genmodel" />
	</extension>
	<extension
         point="org.eclipse.urischeme.uriSchemeHandlers">
      <uriSchemeHandler
            class="org.osate.sysmlv2.contract.uriHandler.gsnURIMarkerHandler"
            uriSchemeDescription="uri marker handler"
            uriScheme="eclipse+com">
      </uriSchemeHandler>
      <uriSchemeHandler
            class="org.osate.sysmlv2.contract.uriHandler.GSNMarkerUriSchemeHandler"
            uriScheme="eclipse+gsnmarker"
            uriSchemeDescription="GSN Markers">
      </uriSchemeHandler>
   </extension>
   	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Sample Marker Handler"
			id="org.osate.sysmlv2.contract.uriHandler.GSNURIMarker"
			name="MarkerHandler">
			<commandParameter
        	id="org.osate.sysmlv2.contract.uriHandler.GSNURIMarker.analysisName"
        	name="par1"/>
		</command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
   		class="org.osate.sysmlv2.contract.uriHandler.GSNURIMarkerCommandHandler"
			commandId="org.osate.sysmlv2.contract.uriHandler.GSNURIMarker">
		</handler>
	</extension>
</plugin>
